{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["lineNumber", "lineName", "from", "to", "stops"],
    "properties": {
      "frequent": {
        "type": "boolean"
      },
      "lineNumber": {
        "type": "integer"
      },
      "lineName": {
        "type": "string"
      },
      "from": {
        "type": "string"
      },
      "to": {
        "type": "string"
      },
      "express": {
        "type": "boolean"
      },
      "ooi-express": {
        "type": "boolean"
      },
      "stops": {
        "type": "object",
        "minProperties": 1,
        "additionalProperties": {
          "type": "string"
        }
      }
    }
  }
  